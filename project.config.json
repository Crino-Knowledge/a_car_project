{
  "miniprogramRoot": "dist/",
  "projectname": "auto-parts-purchase-miniapp",
  "description": "汽服配件采购报价系统 - 门店端",
  "appid": "touristappid",
  "setting": {
    "urlCheck": false,
    "es6": false,
    "enhance": false,
    "compileHotReLoad": false,
    "postcss": false,
    "minified": false,
    "babelSetting": {
      "ignore": [],
      "disablePlugins": [],
      "outputPath": ""
    },
    "useCompilerModule": true,
    "userConfirmedUseCompilerModuleSwitch": false,
    "minifyWXSS": false,
    "minifyWXML": false,
    "ignoreUploadUnusedFiles": true,
    "ignoreDevUnusedFiles": false
  },
  "compileType": "miniprogram",
  "libVersion": "3.3.4",
  "packOptions": {
    "ignore": [
      {
        "type": "file",
        "value": ".eslintrc.js"
      },
      {
        "type": "file",
        "value": "tsconfig.json"
      }
    ]
  },
  "condition": {},
  "editorSetting": {
    "tabIndent": "insertSpaces",
    "tabSize": 2
  },
  "simulatorPluginLibVersion": {}
}
